<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-lazy-init="false">
	
	<bean name="subjectMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="1" value="TKGZYW"></entry>
				<entry key="2" value="TKGZSX"></entry>
				<entry key="3" value="TKGZYY"></entry>
				<entry key="4" value="TKGZWL"></entry>
				<entry key="5" value="TKGZHX"></entry>
				<entry key="6" value="TKGZZZ"></entry>
				<entry key="7" value="TKGZLS"></entry>
				<entry key="8" value="TKGZDL"></entry>
				<entry key="9" value="TKGZSW"></entry>
				<entry key="10" value="TKCZYW"></entry>
				<entry key="11" value="TKCZSX"></entry>
				<entry key="12" value="TKCZYY"></entry>
				<entry key="13" value="TKCZWL"></entry>
				<entry key="14" value="TKCZHX"></entry>
				<entry key="15" value="TKCZZZ"></entry>
				<entry key="16" value="TKCZLS"></entry>
				<entry key="17" value="TKCZDL"></entry>
				<entry key="18" value="TKCZSW"></entry>
				<entry key="19" value="TKXXYW"></entry>
				<entry key="20" value="TKXXSX"></entry>
				<entry key="21" value="TKXXYY"></entry>
			</map>
		</constructor-arg>
	</bean>
	<bean name='wordTypeSet' class='java.util.HashSet'>
		<constructor-arg>
			<set>
				<value>字</value>
				<value>词</value>
			</set>
		</constructor-arg>
	</bean>
	<bean name='wordPropertySet' class='java.util.HashSet'>
		<constructor-arg>
			<set>
				<value>名词</value>
				<value>动词</value>
				<value>形容词</value>
				<value>数词</value>
				<value>量词</value>
				<value>代词</value>
				<value>副词</value>
				<value>介词</value>
				<value>连词</value>
				<value>助词</value>
				<value>叹词</value>
			</set>
		</constructor-arg>
	</bean>
	<bean name='englishWordPropertySet' class='java.util.HashSet'>
		<constructor-arg>
			<set>
				<value>n</value>
				<value>pron</value>
				<value>adj</value>
				<value>num</value>
				<value>v</value>
				<value>adv</value>
				<value>art</value><!-- 冠词 -->
				<value>prep</value><!-- 介词 -->
				<value>conj</value><!-- 连词 -->
				<value>int</value><!-- 叹词 -->
			</set>
		</constructor-arg>
	</bean>
	<bean name='nameTypeSet' class='java.util.HashSet'>
		<constructor-arg>
			<set>
				<value>人名</value>
				<value>地名</value>
			</set>
		</constructor-arg>
	</bean>
	<bean name="typeIndexMap" class="java.util.HashMap">
		<constructor-arg>
			<map>
				<entry key="1" value="一"></entry>
				<entry key="2" value="二"></entry>
				<entry key="3" value="三"></entry>
				<entry key="4" value="四"></entry>
				<entry key="5" value="五"></entry>
				<entry key="6" value="六"></entry>
				<entry key="7" value="七"></entry>
				<entry key="8" value="八"></entry>
				<entry key="9" value="九"></entry>
				<entry key="10" value="十"></entry>
				<entry key="11" value="十一"></entry>
				<entry key="12" value="十二"></entry>
				<entry key="13" value="十三"></entry>
				<entry key="14" value="十四"></entry>
				<entry key="15" value="十五"></entry>
				<entry key="16" value="十六"></entry>
				<entry key="17" value="十七"></entry>
				<entry key="18" value="十八"></entry>
				<entry key="19" value="十九"></entry>
				<entry key="20" value="二十"></entry>
			</map>
		</constructor-arg>
	</bean>
</beans>